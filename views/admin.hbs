<h1>
  Strefa administratora -
  {{appData.login}}
</h1>
<div class="admin-form-add-user">
  <form action="/userAdd" method="POST">
    <label for="first_name">
      Imię
    </label>
    <input type="text" name="first_name" />
    <label for="last_name">
      Nazwisko
    </label>
    <input type="text" name="last_name" />
    <label for="login">
      Login
    </label>
    <input type="text" name="login" />
    <label for="password">
      Hasło
    </label>
    <input type="text" name="password" />
    <label for="date_of_birth_child">
      Data spodziewanych narodzin dziecka
    </label>
    <input type="date" name="date_of_birth_child" />
    <label for="is_admin">
      Prawa administratora?
    </label>
    <input type="checkbox" name="is_admin" value="1" />
    <button type="submit">
      Dodaj użytkownika
    </button>
  </form>
</div>